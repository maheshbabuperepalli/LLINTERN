<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }

        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f2f2f2;
        }

        tr:hover {
            background-color: #f5f5f5;
        }

        p {
            margin-top: 20px;
        }
    </style>
    <title>Document</title>
</head>
<body>
    <% if (stud_attempted.length > 0) { %>
        <table>
            <tr>
                <th>Quiz Id</th>
                <th>Student ID</th>
                <th>Score</th>
            </tr>
            <% stud_attempted.forEach(attempt => { %>
                <tr>
                    <td><%= attempt.quizId %></td>
                    <td><%= attempt.studentID %></td>
                    <td><%= attempt.score %></td>
                </tr>
            <% }); %>
        </table>
    <% } else { %>
        <p>No quiz attempts found.</p>
    <% } %>
</body>
</html>